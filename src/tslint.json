{
  "extends": "../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "app",
      "camelCase"
    ],
    "variable-name": [ false, "ban-keywords", "check-format", "allow-leading-underscore"],
    "no-redundant-jsdoc": false,
    "component-selector": [
      true,
      "element",
      "app",
      "kebab-case"
    ]
  }
}
